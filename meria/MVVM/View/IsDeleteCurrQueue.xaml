<Window x:Class="meria.MVVM.View.IsDeleteCurrQueue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" WindowStyle="None" WindowStartupLocation="CenterScreen"
        Title="IsDeleteCurrQueue" Height="200" Width="400">
    <Grid Background="#3C3C3C">
        <TextBlock x:Name="UserName" Margin="0 0 0 90" FontSize="30" FontWeight="DemiBold" TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" Text="Удалить из очереди"/>
        <Button Click="Button_Click_1" FontSize="25" Content="Нет" Foreground="#887253" BorderThickness="0" Background="#252526" Margin="50 35" Width="120" Height="40" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
        <Button Click="Button_Click" FontSize="25" Content="Да" Foreground="#887253" BorderThickness="0" Background="#252526" Margin="50 35" Width="120" Height="40" HorizontalAlignment="Right" VerticalAlignment="Bottom"/>
    </Grid>
</Window>
